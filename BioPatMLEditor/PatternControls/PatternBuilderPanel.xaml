                        </ScrollViewer>

                    </Border>

                </Border>

                <TextBlock Height="21" FontSize="13.333" FontWeight="Bold" Foreground="#FFD6D6D6" TextWrapping="Wrap" Width="164" Canvas.Left="20" Canvas.Top="45" FontStyle="Italic"><Run Text="Available "/><Run Text="Patterns"/></TextBlock>

            </Canvas>
        </ScrollViewer>

    </Grid>
