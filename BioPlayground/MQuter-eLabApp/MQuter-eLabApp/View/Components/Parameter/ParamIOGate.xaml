<UserControl x:Class="MQuter_eLabApp.View.Components.Parameter.ParamIOGate"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
            >

    <Rectangle x:Name="ParamConnecctor" Stroke="Black" Height="20" Width="21" RadiusX="20" RadiusY="20" Cursor="Hand">
        <Rectangle.Fill>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF265AC2" Offset="0.513" x:Name="GateBrush" />
                <GradientStop Color="#FF0056FF" Offset="1"/>
            </LinearGradientBrush>
        </Rectangle.Fill>
    </Rectangle>
</UserControl>
